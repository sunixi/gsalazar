			[Bindable]

			public var TitleImg:Class;

			[Embed(source="assets/images/CandySplash.png")]

			[Bindable]
			public var SplashImg:Class;

			

			private function onCreationComplete(p_evt:Event):void {

				manualProgressBar.setProgress(40,100);

			}

			

			private function onHeaderLink(p_evt:MouseEvent):void {

				var req:URLRequest = new URLRequest("http://www.philterdesign.com/");

				navigateToURL(req);

			}

			

			private function onTreeSelect(p_evt:Event):void {

				if ((p_evt.currentTarget.selectedItem as XML).children().length() > 0) { return; }

				previewStack.selectedChild = this[p_evt.currentTarget.selectedItem.@label + "Preview"];

			}

		]]>
