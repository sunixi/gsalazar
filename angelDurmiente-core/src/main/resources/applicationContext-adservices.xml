<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN"
    "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="estadisticaService" parent="txProxyTemplate">
		<property name="target">
			<bean class="ar.com.angelDurmiente.services.impl.EstadisticaServiceImpl"/>
		</property>
	</bean>

	<bean id="dedicatoriaService" parent="txProxyTemplate">
		<property name="target">
			<bean class="ar.com.angelDurmiente.services.impl.DedicatoriaServiceImpl">
				<property name="genericDAO" ref="dedicatoriaDAO"/>
			</bean>
		</property>
	</bean>

	<bean id="artistaService" parent="txProxyTemplate">
		<property name="target">
			<bean class="ar.com.angelDurmiente.services.impl.ArtistaServiceImpl">
				<property name="genericDAO" ref="artistaDAO"/>
			</bean>
		</property>
	</bean>

	<bean id="albumService" parent="txProxyTemplate">
		<property name="target">
			<bean class="ar.com.angelDurmiente.services.impl.AlbumServiceImpl">
				<property name="genericDAO" ref="albumDAO"/>
			</bean>
		</property>
	</bean>

	<bean id="cancionService" parent="txProxyTemplate">
		<property name="target">
			<bean class="ar.com.angelDurmiente.services.impl.CancionServiceImpl">
				<property name="genericDAO" ref="cancionDAO"/>
			</bean>
		</property>
	</bean>

</beans>